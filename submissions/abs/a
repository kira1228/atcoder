#include <stdio.h>
int main(void)
{
    int a;
    scanf("%d", &a);
    int count = 0;
    for(int i =0; i < 3; i++) {
        if (a % 10 == 1) {
            count += 1;
            a /= 10;
        }
        if (a % 10 == 0) {
            a /= 10;
        }
    }
    printf("%d", count);
 